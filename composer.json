{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "arcanedev/log-viewer": "^4.5",
    "bacon/bacon-qr-code": "1.0.3",
    "barryvdh/laravel-ide-helper": "^2.4",
    "buzz/laravel-google-captcha": "^2.1",
    "carbondate/carbon": "1.27.0.1",
    "doctrine/dbal": "^2.7",
    "fideloper/proxy": "~4.0",
    "hexters/coinpayment": "^1.1",
    "intervention/image": "^2.4",
    "laravel/framework": "5.6.*",
    "laravel/passport": "^6.0",
    "laravelcollective/html": "^5.6",
    "league/omnipay": "^3.0",
    "maatwebsite/excel": "^2.1",
    "mpdf/mpdf": "^7.0",
    "nesbot/carbon": "1.25.*",
    "omnipay/stripe": "^3.0",
    "paypal/rest-api-sdk-php": "*",
    "phpmailer/phpmailer": "^6.0",
    "pragmarx/google2fa-laravel": "^0.2.0",
    "yajra/laravel-datatables-buttons": "^3.2",
    "yajra/laravel-datatables-html": "^3.7",
    "yajra/laravel-datatables-oracle": "~8.0"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Infoamin\\Installer\\": "packages/infoamin/laravel-installer/src"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
